<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gfk.business.system.project.mapper.ProjectMapper">

    <update id="updateProjectPic">
        update project
        set pic_url  = #{url1},
            pic_url2 = #{url2}
        where id = #{projectId}
    </update>

</mapper>